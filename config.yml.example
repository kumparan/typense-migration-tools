log_level: "debug"
http_connection_settings:
  timeout: "10s"
  tls_handshake_timeout: "5s"
migration:
  source:
    collection: "collection_a"
    typesense:
      host: "http://localhost:8108"
      api_key: "your-api-key"
  destination:
    collection: "collection_b"
    typesense:
      host: "http://localhost:8108"
      api_key: "your-api-key"
  batch_size: "100"
  sorter: "created_at:desc"
  filter: "created_at:<1488325530496000000"
  include_fields:
    - "field1"
    - "field2"
    - "field3"
  exclude_fields:
    - "out_of"
backup:
  typesense:
    host: "http://localhost:8108"
    api_key: "your-api-key"
  batch_size: "100"
  collection: "collection_name"
  jsonl_file_path: "path/to/example.jsonl"
  filter: "created_at:<1488325530496000000"
  include_fields:
    - "field1"
    - "field2"
    - "field3"
  exclude_fields:
    - "out_of"
restore:
  typesense:
    host: "http://localhost:8108"
    api_key: "your-api-key"
  collection: "collection_name"
  jsonl_file_path: "path/to/example.jsonl"
  batch_size: "100"
delete_collection:
  typesense:
    host: "http://localhost:8108"
    api_key: "your-api-key"
  collection: "collection_name"
  batch_size: "100"
  sorter: "created_at:asc"
  sleep_interval: "1s"
  exclude_fields:
    - "out_of"
